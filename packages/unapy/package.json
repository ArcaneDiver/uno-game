{
  "name": "unapy",
  "version": "0.0.0",
  "description": "The UNO Game API",
  "main": "dist/index.js",
  "scripts": {
    "start": "node -r module-alias/register dist/unapy/src/index.js",
    "dev": "nodemon src/index.ts",
    "dev:resources": "docker-compose up",
    "build": "rm -rf dist && tsc && cp -r src/Assets dist/unapy/src/Assets"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.1",
    "@types/socket.io": "^2.1.8",
    "@types/uuid": "^8.0.0",
    "nodemon": "^2.0.4",
    "sucrase": "^3.15.0",
    "tsconfig-paths": "^3.9.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "socket.io": "^2.3.0",
    "typescript": "^3.9.2"
  },
  "_moduleAliases": {
    "@unapy": "dist/unapy/src",
    "@shared/protocols": "packages/shared/protocols",
    "@shared/error-handler": "packages/shared/error-handler/src"
  }
}
